<script type="module">
const globalStatsEl = document.getElementById('global-stats-data');
const trendingEl = document.getElementById('trending-coins');
const aiListEl = document.getElementById('ai-recommendations-list');

function showLoading(el, show) {
    const spinner = el.querySelector('.spinner');
    if (spinner) spinner.style.display = show ? 'inline-block' : 'none';
}

function saveToCache(key, data) {
    try { localStorage.setItem(key, JSON.stringify(data)); } 
    catch (e) { console.warn("Failed to save to cache:", e); }
}

function loadFromCache(key) {
    try { const cached = localStorage.getItem(key); return cached ? JSON.parse(cached) : null; } 
    catch (e) { console.warn("Failed to load from cache:", e); return null; }
}

async function fetchGlobalStats() {
    showLoading(globalStatsEl, true);
    const cached = loadFromCache('globalStats');
    if (cached && !navigator.onLine) {
        globalStatsEl.innerHTML = cached;
        showLoading(globalStatsEl, false);
        return;
    }
    try {
        const res = await fetch("https://api.coingecko.com/api/v3/global");
        if (!res.ok) throw new Error(`API error: ${res.status}`);
        const data = await res.json();
        const stats = data.data;
        const html = `
            <div class="stat-item">Total Market Cap: <div class="stat-value">$${Number(stats.total_market_cap.usd).toLocaleString()}</div></div>
            <div class="stat-item">24h Volume: <div class="stat-value">$${Number(stats.total_volume.usd).toLocaleString()}</div></div>
            <div class="stat-item">BTC Dominance: <div class="stat-value">${stats.market_cap_percentage.btc.toFixed(2)}%</div></div>
            <div class="stat-item">Active Cryptos: <div class="stat-value">${stats.active_cryptocurrencies}</div></div>
        `;
        globalStatsEl.innerHTML = html;
        saveToCache('globalStats', html);
    } catch(err) {
        globalStatsEl.innerHTML = `<p>Failed to load global stats. ${navigator.onLine ? 'Check API or try again.' : 'You are offline—data may be cached.'}</p>`;
    } finally { showLoading(globalStatsEl, false); }
}

async function fetchTrendingCoins() {
    showLoading(trendingEl, true);
    const cached = loadFromCache('trendingCoins');
    if (cached && !navigator.onLine) { trendingEl.innerHTML = cached; showLoading(trendingEl, false); return; }
    try {
        const res = await fetch("https://api.coingecko.com/api/v3/search/trending");
        if (!res.ok) throw new Error(`API error: ${res.status}`);
        const data = await res.json();
        let html = "";
        data.coins.forEach(c => {
            const coin = c.item;
            html += `
                <div class="trending-card card">
                    <img src="${coin.large}" alt="${coin.name}">
                    <h4>${coin.name}</h4>
                    <p>${coin.symbol.toUpperCase()}</p>
                </div>
            `;
        });
        trendingEl.innerHTML = html;
        saveToCache('trendingCoins', html);
    } catch(err) {
        trendingEl.innerHTML = `<p>Failed to load trending coins. ${navigator.onLine ? 'Check API or try again.' : 'You are offline—data may be cached.'}</p>`;
    } finally { showLoading(trendingEl, false); }
}

async function fetchAIPicks() {
    showLoading(aiListEl, true);
    const cached = loadFromCache('aiPicks');
    if (cached && !navigator.onLine) { aiListEl.innerHTML = cached; showLoading(aiListEl, false); return; }
    try {
        const res = await fetch("https://api.coingecko.com/api/v3/coins/markets?vs_currency=usd&order=price_change_percentage_30d_desc&per_page=100&page=1&price_change_percentage=30d");
        if (!res.ok) throw new Error(`API error: ${res.status}`);
        const data = await res.json();
        const topPicks = data.filter(coin => coin.price_change_percentage_30d_in_currency > 0).slice(0, 3);
        let html = "";
        if (topPicks.length === 0) html = `<p>No trending coins found with positive 30-day changes.</p>`;
        else topPicks.forEach(coin => {
            html += `
                <div class="ai-card">
                    <img src="${coin.image}" alt="${coin.name}">
                    <h4>${coin.name}</h4>
                    <p>Price: $${coin.current_price.toLocaleString()}</p>
                    <p>30d Change: ${coin.price_change_percentage_30d_in_currency?.toFixed(2)}%</p>
                    <div class="change-box up">
                      +${coin.price_change_percentage_30d_in_currency.toFixed(2)}%
                    </div>
                </div>
            `;
        });
        aiListEl.innerHTML = html;
        saveToCache('aiPicks', html);
    } catch(err) {
        aiListEl.innerHTML = `<p>Failed to load AI picks. ${navigator.onLine ? 'Check API or try again.' : 'You are offline—data may be cached.'}</p>`;
    } finally { showLoading(aiListEl, false); }
}

fetchGlobalStats();
fetchTrendingCoins();
fetchAIPicks();
</script>

</body>
</html>
le=1.0">
