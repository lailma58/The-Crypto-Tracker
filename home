    } finally { showLoading(globalStatsEl, false); }
}

async function fetchTrendingCoins() {
    showLoading(trendingEl, true);
    const cached = loadFromCache('trendingCoins');
    if (cached && !navigator.onLine) { trendingEl.innerHTML = cached; showLoading(trendingEl, false); return; }
